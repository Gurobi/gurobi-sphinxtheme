\ProvidesPackage{gurobidocs}

% Replacement maketitle block for sphinx latex builds. Based on the existing
% \sphinxmaketitle command generated by sphinx 7.2.6.
%
% Changes:
%
% - Replaces \sphinxlogo with an explicit includegraphics
% - Replaces version label with contents of \GRBversionlabel
% - Adds "Copyright <YYYY>" before the author line
% - Adds a revision label after the date with contents of \GRBdocrevision
%
\newcommand{\grbmaketitle}{%
  \let\sphinxrestorepageanchorsetting\relax
  \ifHy@pageanchor\def\sphinxrestorepageanchorsetting{\Hy@pageanchortrue}\fi
  \hypersetup{pageanchor=false}% avoid duplicate destination warnings
  \begin{titlepage}%
    \let\footnotesize\small
    \let\footnoterule\relax
    \noindent\rule{\textwidth}{1pt}\par
      \begingroup % for PDF information dictionary
       \def\endgraf{ }\def\and{\& }%
       \pdfstringdefDisableCommands{\def\\{, }}% overwrite hyperref setup
       \hypersetup{pdfauthor={\@author}, pdftitle={\@title}}%
      \endgroup
    \begin{flushright}%
      \sphinxincludegraphics[width=0.5\textwidth]{gurobipdflogo.png}
      \par
      \vfill
      \py@HeaderFamily
      {\Huge \@title \par}
      {\itshape\LARGE \GRBversionlabel \par}
      \vfill
      {\large
        \begin{tabular}[t]{c}
          Copyright \copyright\ \the\year{}, \@author
        \end{tabular}\kern-\tabcolsep
        \par}
      \vfill\vfill
      {\large
       \@date \par
       \GRBdocrevision \par
       \vfill
       \py@authoraddress \par
      }%
    \end{flushright}%\par
    \@thanks
  \end{titlepage}%
  \setcounter{footnote}{0}%
  \let\thanks\relax\let\maketitle\relax
  %\gdef\@thanks{}\gdef\@author{}\gdef\@title{}
  \clearpage
  \ifdefined\sphinxbackoftitlepage\sphinxbackoftitlepage\fi
  \if@openright\cleardoublepage\else\clearpage\fi
  \sphinxrestorepageanchorsetting
}
